    "@tarojs/components": "4.0.2",
    "@tarojs/helper": "4.0.2",
    "@tarojs/plugin-framework-react": "4.0.2",
    "@tarojs/plugin-platform-weapp": "^4.0.2",
    "@tarojs/react": "^4.1.5",
    "@tarojs/runtime": "4.0.2",
    "@tarojs/shared": "4.0.2",
    "@tarojs/taro": "4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
