    "@types/react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-preset-taro": "4.0.2",
    "eslint": "^8.12.0",
    "eslint-config-taro": "4.0.2",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.6.0",
